<%@ params(int index, int size) %>
<div>
<% if (index > 0) {%>
<button onclick="location.href='/reviews/${index-1}'" type="button">&lt; Previous</button>
<% } %>
<button onclick="location.href='/reviews'" type="button">^List^</button>
<% if (index < size-1) {%>
<button onclick="location.href='/reviews/${index+1}'" type="button">Next &gt;</button>
<% } %>
</div>
